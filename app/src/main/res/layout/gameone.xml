<?xml version="1.0" encoding="UTF-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ScrollView
        android:layout_height="862dp"
        android:layout_width="543dp"
        app:layout_constraintVertical_bias="0.503"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/scrollView2">

    <LinearLayout
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical"/>

</ScrollView>

    <TextView
        android:id="@+id/TitleOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Quick Maths!"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        android:id="@+id/textView3" android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="â€“"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        android:id="@+id/AnswerOne"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="Y"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        android:id="@+id/imageView"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        tools:srcCompat="@tools:sample/avatars"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        android:id="@+id/imageView2"
        app:layout_constraintStart_toEndOf="@+id/NumberOne"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        tools:srcCompat="@tools:sample/avatars"
        app:layout_constraintTop_toBottomOf="@+id/TitleOne"
        android:layout_marginTop="32dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/imageView2"
        android:id="@+id/NumberOne"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="X"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        android:id="@+id/textView5"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/imageView"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        app:layout_constraintStart_toStartOf="@+id/imageView2"
        android:id="@+id/imageView3"
        tools:srcCompat="@tools:sample/avatars"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/textView3"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintStart_toStartOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="@+id/imageView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        android:id="@+id/imageView4"
        tools:srcCompat="@tools:sample/avatars"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView3"
        android:id="@+id/textView6"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="+"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4"
        android:id="@+id/textView7"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        android:id="@+id/AnswerTwo"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="Z"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/TitleOne"
        app:layout_constraintEnd_toEndOf="@+id/TitleOne"
        android:id="@+id/textView"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        android:text="Solve For:"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        android:layout_marginTop="32dp"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        android:id="@+id/imageView6"
        tools:srcCompat="@tools:sample/avatars"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginTop="16dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        android:id="@+id/textView2"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        android:id="@+id/textView4"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:layout_height="75dp"
        android:layout_width="75dp"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="@+id/imageView6"
        android:id="@+id/imageView7"
        tools:srcCompat="@tools:sample/avatars"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        android:layout_marginTop="32dp"/>

    <EditText
        android:layout_height="75dp"
        android:layout_width="132dp"
        app:layout_constraintTop_toTopOf="@+id/textView2"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        android:id="@+id/InputOne" android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:inputType="number"
        android:ems="10"/>

    <EditText
        android:layout_height="75dp"
        android:layout_width="132dp"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        android:id="@+id/InputTwo"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
        android:inputType="number"
        android:ems="10"/>

    <Button
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        android:id="@+id/EnterAnswer"
        android:text="Enter"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        android:layout_marginTop="16dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/EnterAnswer"
        app:layout_constraintEnd_toEndOf="@+id/EnterAnswer"
        android:id="@+id/Message"
        app:layout_constraintTop_toBottomOf="@+id/EnterAnswer"
        android:layout_marginTop="8dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>