<?xml version="1.0" encoding="UTF-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="543dp"
        android:layout_height="862dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.419">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" />

    </ScrollView>

    <TextView
        android:id="@+id/G1tv_Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Quick Maths!"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Appleone"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Appleone"
        android:id="@+id/textView3" android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="â€“"
        app:layout_constraintStart_toEndOf="@+id/G1iv_Appleone"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Orangeone"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Orangeone"
        android:id="@+id/G1tv_Answerone"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="Y"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:id="@+id/G1iv_Orangeone"
        android:layout_width="65dp"
        android:layout_height="75dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Appleone"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/G1iv_Appleone"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toEndOf="@+id/G1tv_Coefficient"
        app:layout_constraintTop_toBottomOf="@+id/G1tv_Title"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Appleone"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Appleone"
        android:id="@+id/G1tv_Coefficient"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="X"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Orangeone"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Orangeone"
        android:id="@+id/textView5"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/G1iv_Orangeone"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:id="@+id/G1iv_Orangetwo"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:layout_marginTop="32dp"
        app:layout_constraintEnd_toStartOf="@+id/textView3"
        app:layout_constraintStart_toStartOf="@+id/G1iv_Appleone"
        app:layout_constraintTop_toBottomOf="@+id/G1iv_Appleone"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/G1iv_Appletwo"
        android:layout_width="65dp"
        android:layout_height="75dp"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Orangetwo"
        app:layout_constraintEnd_toEndOf="@+id/G1iv_Orangeone"
        app:layout_constraintStart_toStartOf="@+id/G1iv_Orangeone"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Orangetwo"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Orangetwo"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Orangetwo"
        android:id="@+id/textView6"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="+"
        app:layout_constraintStart_toEndOf="@+id/G1iv_Orangetwo"
        app:layout_constraintEnd_toStartOf="@+id/G1iv_Appletwo"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_Appletwo"
        app:layout_constraintStart_toStartOf="@+id/textView5"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_Appletwo"
        android:id="@+id/textView7"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        android:id="@+id/G1tv_Answertwo"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="Z"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Solve For:"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintEnd_toEndOf="@+id/G1tv_Title"
        app:layout_constraintStart_toStartOf="@+id/G1tv_Title"
        app:layout_constraintTop_toBottomOf="@+id/G1iv_Appletwo" />

    <ImageView
        android:id="@+id/G1iv_FinalApple"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/G1iv_Orangetwo"
        app:layout_constraintStart_toStartOf="@+id/G1iv_Orangetwo"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_FinalApple"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_FinalApple"
        android:id="@+id/textView2"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/G1iv_FinalApple"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <TextView
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:layout_constraintTop_toTopOf="@+id/G1iv_FinalOrange"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_FinalOrange"
        android:id="@+id/textView4"
        android:textAppearance="@style/TextAppearance.AppCompat.Display3"
        android:text="="
        app:layout_constraintStart_toEndOf="@+id/G1iv_FinalApple"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"/>

    <ImageView
        android:id="@+id/G1iv_FinalOrange"
        android:layout_width="54dp"
        android:layout_height="54dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/G1iv_FinalApple"
        app:layout_constraintStart_toStartOf="@+id/G1iv_FinalApple"
        app:layout_constraintTop_toBottomOf="@+id/G1iv_FinalApple"
        tools:srcCompat="@tools:sample/avatars" />

    <EditText
        android:id="@+id/G1et_Inputapple"
        android:layout_width="115dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:ems="10"
        android:inputType="number"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="@+id/textView2" />

    <EditText
        android:id="@+id/G1et_Inputorange"
        android:layout_width="115dp"
        android:layout_height="60dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:ems="10"
        android:inputType="number"
        android:textAppearance="@style/TextAppearance.AppCompat.Display2"
        app:layout_constraintBottom_toBottomOf="@+id/G1iv_FinalOrange"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/G1iv_FinalOrange" />

    <Button
        android:id="@+id/G1bt_Enter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Enter"
        app:layout_constraintEnd_toEndOf="@+id/textView"
        app:layout_constraintStart_toStartOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/G1tv_Message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="@+id/G1bt_Enter"
        app:layout_constraintStart_toStartOf="@+id/G1bt_Enter"
        app:layout_constraintTop_toBottomOf="@+id/G1bt_Enter" />

</androidx.constraintlayout.widget.ConstraintLayout>